<core:FragmentDefinition xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
								<f:Form id="idQuantForm" editable="true">
									<f:layout>
										<f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4"
											emptySpanM="4" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false"/>
									</f:layout>
									<f:formContainers>
										<f:FormContainer>
											<f:formElements>
												<f:FormElement label="Qty in Unit Entry">
													<f:fields>
														<Input id="ident" editable="{EditModel>/isEditable}" value=""/>
														<Input id="idQunat" editable="{EditModel>/isEditable}" value="">
															<layoutData>
																<l:GridData span="XL1 L2 M2 S4"/>
															</layoutData>
														</Input>
													</f:fields>
												</f:FormElement>
												<f:FormElement label="Qty in SKU">
													<f:fields>
														<Input id="idQntS" editable="{EditModel>/isEditable}" value=""/>
														<Input id="idQuantsku" editable="{EditModel>/isEditable}" value="">
															<layoutData>
																<l:GridData span="XL1 L2 M2 S4"/>
															</layoutData>
														</Input>
													</f:fields>
												</f:FormElement>
												<f:FormElement label="Qty in Delivery note">
													<f:fields>
														<Input id="idDelNot" editable="{EditModel>/isEditable}" value=""/>
														<Input id="idDelNott" editable="{EditModel>/isEditable}" value="">
															<layoutData>
																<l:GridData span="XL1 L2 M2 S4"/>
															</layoutData>
														</Input>
													</f:fields>
												</f:FormElement>
											</f:formElements>
										</f:FormContainer>
									</f:formContainers>
								</f:Form>
							</VBox>
</core:FragmentDefinition>