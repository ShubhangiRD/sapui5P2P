<core:FragmentDefinition xmlns="sap.m" xmlns:t="sap.ui.table" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="TableBoxClass sapUiSmallMargin">
		<t:Table id="idTableitem" rows="{ path: 'GoodReceiptModel>/GRPost/GrItemSet'}" editable="true" visibleRowCount="4"
			selectionMode="MultiToggle">
			<t:extension>
				<OverflowToolbar>
					<Title text=" Purchase Order Items"/>
					<ToolbarSpacer/>
				<!--	<Button icon="sap-icon://delete" enabled="{=${GoodReceiptModel>/TempContract/RebateConditionItems}.length > 0}" type="Transparent"
						tooltip="Delete Row" press="onDeleteConditionItem"/>-->
				</OverflowToolbar>
			</t:extension>
			<t:columns>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Line"/>
					<t:template>
						<Input id="idMaterialNumber" textFormatMode="KeyValue" editable="{EditModel>/isEditable}" value="{GoodReceiptModel>LineId}"></Input>
					</t:template>
				</t:Column>
				<t:Column hAlign="Center" width="12rem">
					<Label text="Material Number"/>
					<t:template>
						<Input class="desc1" id="Material" value="{GoodReceiptModel>Material}" editable="{EditModel>/isEditable}"></Input>
					</t:template>
				</t:Column>
						<t:Column width="7rem" hAlign="Center">
					<Label text="Quantity"/>
					<t:template>
						<!--	<u:Currency id="Quantity" textAlign="Center" stringValue="{GoodReceiptModel>Quantity}" editable="{EditModel>/isEditable}" currency="{ }" useSymbol="true"/>
					-->
						<Input id="Quantity" value="{GoodReceiptModel>PoPrQnt}" editable="{EditModel>/isEditable}" textAlign="End"/>
					</t:template>
				</t:Column>
						<t:Column width="4rem" hAlign="Center">
					<Label text="EUn"/>
					<t:template>
						<Input id="idEUn" value="{GoodReceiptModel>EntryUom}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
			<!--	<t:Column hAlign="Center" width="12rem">
					<Label text="Material Description"/>
					<t:template>
						<Input class="desc1" id="nDescription" value="{GoodReceiptModel>ShortText}" editable="{EditModel>/isEditable}"></Input>
					</t:template>
				</t:Column>-->
				<t:Column width="4rem" hAlign="Center">
					<Label text="ISO Code"/>
					<t:template>
						<Input id="idEntryUomIso" value="{GoodReceiptModel>EntryUomIso}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="9rem" hAlign="Center">
					<Label text="Vendor"/>
					<t:template>
						<Input id="Price" value="{GoodReceiptModel>Vendor}" class="price1" editable="false"/>
					</t:template>
				</t:Column>
				<t:Column width="9rem" hAlign="Center">
					<Label text="Storage Location"/>
					<t:template>
						<Input id="idCust" value="{GoodReceiptModel>StgeLoc}" editable="{EditModel>/isEditable}" textFormatMode="KeyValue"
							placeholder="Storage Location" showSuggestion="true" showValueHelp="true" valueHelpRequest="handleStorageLocationValue"
							suggestionItems="{ path: 'Lookup>/StorageLocationList'}" suggestionItemSelected="suggestionItemStorageLocation">
							<suggestionItems>
								<core:ListItem key="{Lookup>Lgort}" text="{Lookup>Lgobe}" additionalText="{Lookup>Lgort}"/>
							</suggestionItems>
						</Input>
					</t:template>
				</t:Column>
				<t:Column width="9rem" hAlign="Center">
					<Label text="Plant"/>
					<t:template>
						<Input id="idPlanttab" value="{GoodReceiptModel>Plant}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
		
				<!--<t:Column width="9rem" hAlign="Center">
					<Label text="NetPrice"/>
					<t:template>
					<Input id="idNetprice" value="{GoodReceiptModel>NetPrice}" textAlign="End" editable="{EditModel>/isEditable}"></Input>
					</t:template>
				</t:Column>-->
				<t:Column width="7rem" hAlign="Center">
					<Label text="Unit of Quantity"/>
					<t:template>
						<Input id="uom1" value="{GoodReceiptModel>PoPrQnt}" editable="{EditModel>/isEditable}" class="measure1"/>
					</t:template>
				</t:Column>
		
				<t:Column width="7rem" hAlign="Center">
					<Label text="Purchase Order"/>
					<t:template>
						<Input id="idPoNumber" value="{GoodReceiptModel>PoNumber}" editable="{EditModel>/isEditable}"  />
					</t:template>
				</t:Column>
			<t:Column width="9rem" hAlign="Center">
					<Label text="Item "/>
					<t:template>
						<Input id="PoItem" value="{GoodReceiptModel>PoItem}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Cost Center"/>
					<t:template>
						<Input id="idCostCenter" value="{GoodReceiptModel>Costcenter}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Partner"/>
					<t:template>
						<Input id="idPArtnr" value="{GoodReceiptModel>PartAcct}" editable="{EditModel>/isEditable}" />
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Profit Center"/>
					<t:template>
						<Input id="idProfitCenter"  value="{GoodReceiptModel>ProfitCtr}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Network"/>
					<t:template>
						<Input id="idGLAccount" value="{GoodReceiptModel>Network}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="5rem" hAlign="Center">
					<Label text="Batch"/>
					<t:template>
						<Input id="idBatch"  value="{GoodReceiptModel>Batch}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Valuation Type"/>
					<t:template>
						<Input id="idValuationType" value="{GoodReceiptModel>ValType}" editable="{EditModel>/isEditable}" />
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Movement Type"/>
					<t:template>
						<Input id="idMovementType1" value="{GoodReceiptModel>MoveType}" editable="{EditModel>/isEditable}"  />
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Direction"/>
					<t:template>
						<Input id="idDirection" showValueHelp="true" showSuggestion="true"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Stock Type"/>
					<t:template>
								<Input id="idStockType" value="{GoodReceiptModel>StckType}" editable="{EditModel>/isEditable}"  />
			
				
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Special Stock"/>
					<t:template>
						<Input id="idSpecialStock" value="{GoodReceiptModel>SpecStock}" editable="{EditModel>/isEditable}" />
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Customer"/>
					<t:template>
						<Input id="idCustomer" value="{GoodReceiptModel>Customer}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
			
				<t:Column width="7rem" hAlign="Center">
					<Label text="Sales Order"/>
					<t:template>
						<Input id="idSalesOrder" value="{GoodReceiptModel>SalesOrd}" editable="{EditModel>/isEditable}" />
					</t:template>
				</t:Column>
					<t:Column width="7rem" hAlign="Center">
					<Label text="Sales Order Item"/>
					<t:template>
						<Input id="idSalesOrderItem" value="{GoodReceiptModel>SOrdItem}" editable="{EditModel>/isEditable}" />
					</t:template>
				</t:Column>
				<t:Column width="9rem" hAlign="Center">
					<Label text="Deliv. Compl."/>
					<t:template>
						<Input id="idNoMoreGr" value="{GoodReceiptModel>NoMoreGr}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="WBS Element"/>
					<t:template>
						<Input id="idWBSElement" value="{GoodReceiptModel>WbsElem}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
			<!--	<t:Column width="7rem" hAlign="Center">
					<Label text="Qty in OPUn"/>
					<t:template>
						<Input id="idQtyInOPUn" value="{GoodReceiptModel>WbsElem}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>-->
				<t:Column width="7rem" hAlign="Center">
					<Label text="OPUn"/>
					<t:template>
						<Input id="idOPUn" value="{GoodReceiptModel>OrderprUn}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Item"/>
					<t:template>
						<Input id="idItem" value="{GoodReceiptModel>ItemText}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="9rem" hAlign="Center">
					<Label text="Material Document "/>
					<t:template>
						<Input id="idDCIPOItem" value="{GoodReceiptModel>MatDoc}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				
			<t:Column width="7rem" hAlign="Center">
					<Label text="Material Document Item" />
					<t:template>
						<Input id="idDateOfManuf" value="{GoodReceiptModel>MatdocItm}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
			
				<t:Column width="7rem" hAlign="Center">
					<Label text="Ref Doc"/>
					<t:template>
						<Input id="idRefDoc" value="{GoodReceiptModel>RefDoc}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
					<t:Column width="7rem" hAlign="Center">
					<Label text="Currency"/>
					<t:template>
						<Input id="idCurrency" value="{GoodReceiptModel>Currency}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="CurrencyIso"/>
					<t:template>
						<Input id="idExtAmountLC" value="{GoodReceiptModel>CurrencyIso}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Reservation Number"/>
					<t:template>
						<Input id="idResno" value="{GoodReceiptModel>ReservNo}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="RealEstet Key"/>
					<t:template>
						<Input id="idRlEstKey" value="{GoodReceiptModel>RlEstKey}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Value Sales Order"/>
					<t:template>
						<Input id="idvsales" value="{GoodReceiptModel>ValSOrdItem}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Document Year"/>
					<t:template>
						<Input id="DocYear" value="{GoodReceiptModel>DocYear}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Document Item"/>
					<t:template>
						<Input id="EarmarkedItem" value="{GoodReceiptModel>EarmarkedItem}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Earmarked Funds"/>
					<t:template>
						<Input id="EarmarkedNumber" value="{GoodReceiptModel>EarmarkedNumber}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Budget Period"/>
					<t:template>
						<Input id="BudgetPeriod" value="{GoodReceiptModel>BudgetPeriod}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Movement ind."/>
					<t:template>
						<Input id="idMPNMaterial" value="{GoodReceiptModel>MvtInd}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="ISO code."/>
					<t:template>
						<Input id="OrderprUnIso" value="{GoodReceiptModel>OrderprUnIso}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Acctg Indicator"/>
					<t:template>
						<Input id="CalcMotive" value="{GoodReceiptModel>CalcMotive}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Business Proc."/>
					<t:template>
						<Input type="Text" id="CoBusproc" value="{GoodReceiptModel>CoBusproc}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Level"/>
					<t:template>
						<Input id="LineDepth" value="{GoodReceiptModel>LineDepth}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Functional Area"/>
					<t:template>
						<Input id="FuncAreaLong" value="{GoodReceiptModel>FuncAreaLong}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
			
				<t:Column width="7rem" hAlign="Center">
					<Label text="Parent Id"/>
					<t:template>
						<Input id="ParentId" value="{GoodReceiptModel>ParentId}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
	
				<t:Column width="7rem" hAlign="Center">
					<Label text="Stock Segment"/>
					<t:template>
						<Input id="StkSegment" value="{GoodReceiptModel>StkSegment}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="Auto. created"/>
					<t:template>
						<Input id="XAutoCre" value="{GoodReceiptModel>XAutoCre}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
		
			
				<t:Column width="7rem" hAlign="Center">
					<Label text="Funds Center" />
					<t:template>
						<Input id="FundsCtr" value="{GoodReceiptModel>FundsCtr}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
			
			
						<t:Column width="7rem" hAlign="Center">
					<Label text="Ref Doc Year"/>
					<t:template>
						<Input id="idRfDocYear" value="{GoodReceiptModel>RefDocYr}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
				<t:Column width="7rem" hAlign="Center">
					<Label text="RfDocIt"/>
					<t:template>
						<Input id="idRfIt" value="{GoodReceiptModel>RefDocIt}" editable="{EditModel>/isEditable}"/>
					</t:template>
				</t:Column>
			
			
			</t:columns>
		</t:Table>
	</VBox>
</core:FragmentDefinition>