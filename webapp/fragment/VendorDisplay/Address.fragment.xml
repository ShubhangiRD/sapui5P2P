				<Label text="Postal Code" fieldGroupIds="Pstlza"/>
				<Input maxLength="6" value="{VendorModel>/Pstlza}" class="InputBoxContainer" change="changePostalCode" id="idPostcode"
					placeholder="Postal Code" editable="{EditModel>/isEditable}"/>
