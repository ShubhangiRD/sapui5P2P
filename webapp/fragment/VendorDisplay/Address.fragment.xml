		<form:SimpleForm id="SimpleFormChange480_12120-2" editable="true" layout="ResponsiveGridLayout" title="Communication Details"
			labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0"
			emptySpanS="0" columnsXL="3" columnsL="3" columnsM="1" class="myFormMargin" singleContainerFullSize="false">
			<form:content>
				<core:Title text=" "/>
				<Label text="Language" fieldGroupIds="Sprasa"/>
				<Input id="idlanguage" value="{VendorModel>/Sprasa}" placeholder="Language" type="Text" showSuggestion="true" maxLength="1"
					showValueHelp="true" valueHelpRequest="handleLanguage" suggestionItems="{ path: 'Lookup>/LanguageList'}"
					suggestionItemSelected="suggestionItemLanguage" editable="{EditModel>/isEditable}">
					<suggestionItems>
						<core:Item key="{Lookup>Sptxt}" text="{Lookup>Sptxt}"/>
					</suggestionItems>
				</Input>
				<Label text="Telphone" fieldGroupIds="Telf1a"/>
				<Input maxLength="10" value="{VendorModel>/Telf1a}" id="idTel" class="InputBoxContainer" editable="{EditModel>/isEditable}"/>
				<Label text="Fax" fieldGroupIds="Telfxa"/>
				<Input maxLength="31" id="idfax" class="InputBoxContainer" value="{VendorModel>/Telfxa}" editable="{EditModel>/isEditable}"/>
				<Label text="Email" fieldGroupIds="Intadb"/>
				<Input id="idEmails" fieldGroupIds="Intadb" class="InputBoxContainer" value="{VendorModel>/Intadb}" editable="{EditModel>/isEditable}"/>
				<core:Title text=" "/>
				<Label text="Data Line" fieldGroupIds="Datlta"/>
				<Input maxLength="14" id="idDataline" value="{VendorModel>/Datlta}" class="InputBoxContainer" placeholder="Enter Data Line"
					editable="{EditModel>/isEditable}"/>
				<Label text="Tel Ext" fieldGroupIds="Teltxa"/>
				<Input id="TelExtension" class="InputBoxContainer" value="{VendorModel>/Teltxa}" placeholder="Enter Extension"
					editable="{EditModel>/isEditable}"/>
				<Label text="Fax Ext"/>
				<Input id="FaxExtension" class="InputBoxContainer" value="{VendorModel>/Telx1a}" placeholder="Enter Extension"
					editable="{EditModel>/isEditable}"/>
				<Label text="Tele Box" fieldGroupIds="Telbxa"/>
				<Input id="TeleboxNumber" class="InputBoxContainer" value="{VendorModel>/Telebox}" placeholder="Enter Extension"
					editable="{EditModel>/isEditable}"/>
			</form:content>
		</form:SimpleForm>
