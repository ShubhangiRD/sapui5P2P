					<items>
						<core:Item text="Mr." key="Mr"/>
						<core:Item text="Ms." key="Ms"/>
						<core:Item text="MADM." key="MADM"/>
						<core:Item text="MISS." key="MISS"/>
						<core:Item text="MIST." key="MIST"/>
						<core:Item text="Company" key="Company"/>
						<core:Item text="DOCT." key="DOCT"/>
						<core:Item text="Dr." key="Dr"/>
					</items>
					<layoutData>
						<l:GridData span="XL2 L3 M4 S4"/>
					</layoutData>
				</Select>-->
				<Input value="{VendorModel>/Name1a}" fieldGroupIds="Name1a" id="idFsname" maxLength="35" placeholder="Firstname"
					editable="{EditModel>/isEditable}"></Input>
				<Label text="LastName" required="true" fieldGroupIds="Name2a"/>
				<Input maxLength="35" value="{VendorModel>/Name2a}" id="idLname" placeholder="LastName" editable="{EditModel>/isEditable}"/>
				<Label text="Search Term" required="true" fieldGroupIds=""/>
				<Input maxLength="25" value="{VendorModel>/Sortla}" id="idSearchTerm" placeholder="Search Term" editable="{EditModel>/isEditable}"/>
				<Label text="Country Code" required="true" fieldGroupIds="Land1a"/>
				<Input maxLength="3" id="idCountryCode" textFormatMode="KeyValue" placeholder="Country Code" showSuggestion="true"
					value="{VendorModel>/Land1a}" showValueHelp="true" valueHelpRequest="handleValueHelpCountryCode"
					suggestionItems="{ path: 'Lookup>/CountryCodeRegion' }" suggestionItemSelected="onSuggest" editable="{EditModel>/isEditable}">
					<suggestionItems>
						<core:ListItem key="{Lookup>Land1}" text="{Lookup>Landx}" additionalText="{Lookup>Land1}"/>
					</suggestionItems>
				</Input>
				<Label text="Time Zone " fieldGroupIds=""/>
				<Input id="TimeZone" type="Text" showSuggestion="true" showValueHelp="true" editable="{EditModel>/isEditable}" value="{VendorModel>/ }"
					valueHelpRequest="handleValueHelpTimeZone" suggestionItems="{ path: 'hierarchy>/results'}" suggestionItemSelected="suggestionItemTimeZone">
					<suggestionItems>
						<core:Item key="{hierarchy>Bezei}" text="{hierarchy>Bland}"/>
					</suggestionItems>
				</Input>
				<Label text="POBxPostalCd" fieldGroupIds="Pstl2a"/>
				<Input maxLength="6" value="{VendorModel>/Pstl2a}" id="idPOBoxPostalCode" editable="{EditModel>/isEditable}"></Input>
				<core:Title text=" "/>
				<Label text="Street" fieldGroupIds="Strasa"/>
				<Input maxLength="35" value="{VendorModel>/Strasa}" id="idStreet" placeholder=" Your Street" editable="{EditModel>/isEditable}"></Input>
				<Label text="House No" fieldGroupIds="Adrnra"/>
				<Input maxLength="10" id="idAddno" value="{VendorModel>/Adrnra}" type="Text" editable="{EditModel>/isEditable}" placeholder="House Number"></Input>
				<Label text="City" fieldGroupIds="Ort01a"/>
				<Input maxLength="35" value="{VendorModel>/Ort01a}" id="idCity" class="InputBoxContainer" placeholder=" City"
					editable="{EditModel>/isEditable}"/>
				<Label text="Region " fieldGroupIds="Regioa"/>
				<Input id="idRegion" maxLength="3" value="{VendorModel>/Regioa}" placeholder="Select Region" type="Text" showSuggestion="true"
