<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Payment Terms List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/PaymentTermsList}"
		search="_handlePaymentTermsPOSearch"
		confirm="_handlePaymentTermsPOClose"
		cancel="_handlePaymentTermsPOClose">
		<StandardListItem
			title="{Lookup>Zterm}"
			info="{Lookup>Text1}" />
	</SelectDialog>
</core:FragmentDefinition>