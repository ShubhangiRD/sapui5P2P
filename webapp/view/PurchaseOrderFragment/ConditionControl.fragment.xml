<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="Conditon" editable="true" class="sapUiTinyMarginTop">
			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
					emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="1" singleContainerFullSize="false"/>
			</f:layout>
		
		
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label=" " class="sapUiTinyMarginTop">
							<f:fields>
								<CheckBox id="printprc" class="sapUiTinyMarginTop" text="Print price"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label=" " class="sapUiTinyMarginTop">
							<f:fields>
								<CheckBox id="idestimatedprc" class="sapUiTinyMarginTop" text="Estimated price"/>
							</f:fields>
						</f:FormElement>
					
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>