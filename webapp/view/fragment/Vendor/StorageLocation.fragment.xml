

<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Storage Location"
		class="sapUiPopupWithPadding"
		items="{Lookup>/StorageLocationList}"
		search="_handleStorageLocationSearch"
		confirm="_handleStorageLocationClose"
		cancel="_handleStorageLocationClose">
		<StandardListItem
			title="{Lookup>Lgort}"
			info="{Lookup>Lgobe}" />
	</SelectDialog>
</core:FragmentDefinition>