<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Language List"
		class="sapUiPopupWithPadding"
		items="{ path: 'Lookup>/LanguageList'}"
		search="_handleValueHelpSearchLanguage"
		confirm="_handleValueHelpCloseLanguage"
		cancel="_handleValueHelpCloseLanguage">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{Lookup>Sptxt}"
			description="{Lookup>Sptxt}" />
	</SelectDialog>
</core:FragmentDefinition>