<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Time Zone List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/TimeZoneList}"
		search="handleValueHelpTimeZoneSearch"
		confirm="handleValueHelpTimeZoneClose"
		cancel="handleValueHelpTimeZoneClose">
		<StandardListItem
			title="{Lookup>Tzone}"
			info="{Lookup>Zonerule}" />
	</SelectDialog>
</core:FragmentDefinition>