<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Transport Zone List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/TransportZoneList}"
		search="_handleTransportZoneSearch"
		confirm="_handleTransportZoneClose"
		cancel="_handleTransportZoneClose">
		<StandardListItem
			title="{Lookup>Zone1}"
			info="{Lookup>Land1}" />
	</SelectDialog>
</core:FragmentDefinition>