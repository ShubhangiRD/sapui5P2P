<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Pricing Date List"
		class="popx sapUiPopupWithPadding"
		items="{pricingdate>/PricingDate}"
		search="_handleValueHelpSearchPrice"
		confirm="_handleValueHelpClosePrice"
		cancel="_handleValueHelpClosePrice">
		<StandardListItem
			title="{pricingdate>pricingno}"
			info="{pricingdate>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>