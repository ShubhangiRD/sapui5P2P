<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Partner Function List"
		class="sapUiPopupWithPadding"
		items="{partnerfunction>/PartnerFunction}"
		search="_handlePartnerFunctionSearch"
		confirm="_handlePartnerFunctionClose"
		cancel="_handlePartnerFunctionClose">
		<StandardListItem
			title="{partnerfunction>function}"
			info="{partnerfunction>Description}" />
	</SelectDialog>
</core:FragmentDefinition>