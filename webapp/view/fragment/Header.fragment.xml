<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:t="sap.ui.table" xmlns:l="sap.ui.layout"
	xmlns:form="sap.ui.layout.form" xmlns:f="sap.f" displayBlock="true" xmlns:tnt="sap.tnt">
	<tnt:ToolHeader>
		<Button icon="sap-icon://menu2" type="Transparent" press="onMenuButtonPress">
			<layoutData>
				<FlexItemData growFactor="1"/>
			</layoutData>
		</Button>
		<Image src="./images/Cassini-logo.png" densityAware="false" class="logo" width="9rem">
			<layoutData>
				<FlexItemData growFactor="1"/>
			</layoutData>
		</Image>
	
		<FlexBox direction="Column" alignItems="Start">
			<items></items>
		</FlexBox>
		<ToolbarSpacer/>
		<FlexBox class="logoContent" direction="Column" alignItems="Start">
			<items>
			
			</items>
		</FlexBox>
		<ToolbarSpacer/>
		<Button icon="sap-icon://comment" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow"/>
			</layoutData>
		</Button>
		<Switch state="true" customTextOn="EN" customTextOff="DE" change="onChangeLanguage">
			<layoutData>
				<FlexItemData growFactor="1"/>
			</layoutData>
		</Switch>
	</tnt:ToolHeader>
</core:FragmentDefinition>