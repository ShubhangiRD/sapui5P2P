<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Plant List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/POPlant}"
		search="_handlePlantSearch"
		confirm="_handlePlantClose"
		cancel="_handlePlantClose">
		<StandardListItem
			title="{Lookup>Bwkey}"
			info="{Lookup>Bwkey}" />
	</SelectDialog>
</core:FragmentDefinition>

