<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Material List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/MaterialList}"
		search="_handlePOMaterialSearch"
		confirm="_handlePOMaterialClose"
		cancel="_handlePOMaterialClose">
		<StandardListItem
			title="{Lookup>Materialno}"
			info="{Lookup>Description}" />
	</SelectDialog>
</core:FragmentDefinition>



