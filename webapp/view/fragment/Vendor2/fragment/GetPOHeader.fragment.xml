<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Vendor PO List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/POVendorList}"
		search="_handlePOVendorSearch"
		confirm="_handlePOVendorHelpClose"
		cancel="_handlePOVendorHelpClose">
		<StandardListItem
			title="{Lookup>Name1}"
			info="{Lookup>Lifnr}" />
	</SelectDialog>
