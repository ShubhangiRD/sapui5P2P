<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Material List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/MaterialList}"
		search="_handleMaterialValueHelpSearch"
		confirm="_handleMaterialValueHelpClose"
		cancel="_handleMaterialValueHelpClose"
		multiSelect="true">
		<StandardListItem
			title="{Lookup>Description}"
			info="{Lookup>MaterialNo}" />
	</SelectDialog>
</core:FragmentDefinition>