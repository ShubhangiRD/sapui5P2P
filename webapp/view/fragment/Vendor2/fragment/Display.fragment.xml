<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Display Vendor List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/DisplyaVendorList}"
		search="_handleMaterialValueHelpSearch"
		confirm="_handleValueHelpClose1"
		cancel="_handleValueHelpClose1">
		<StandardListItem
			title="{Lookup>Name1}"
			info="{Lookup>Lifnr}" />
	</SelectDialog>
</core:FragmentDefinition>



