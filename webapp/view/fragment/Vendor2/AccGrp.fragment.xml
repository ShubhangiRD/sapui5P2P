		title="Account Group List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/AccountGroup}"
		search="_handlevendorAccountGSearch"
		confirm="_handlevendorAccountGClose"
		cancel="_handlevendorAccountGClose">
