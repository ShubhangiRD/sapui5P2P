<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title=" Country List"
		class="sapUiPopupWithPadding"
		items="{Lookup>/CountryCodeRegion}"
		search="_handlecountryVendorSearch"
		confirm="_handlecountryVendorClose"
		cancel="_handlecountryVendorClose">
		<StandardListItem
			title="{Lookup>Land1}"
			info="{Lookup>Landx}" />
	</SelectDialog>
</core:FragmentDefinition>
